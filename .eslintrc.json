{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "strict":"off",
        "comma-dangle":"off",
        "no-underscore-dangle":"off",
        "linebreak-style":"off",
        "semi":"off",
        "object-curly-newline":"off"
    }
}
